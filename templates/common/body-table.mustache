<div class="table">
    {{^hidePeople}}
        <table>
            <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
                <th>City</th>
            </tr>
            </thead>
            <tbody>
            {{#people}}
                <tr>
                    <td>{{ FirstName }}</td>
                    <td>{{ LastName }}</td>
                    <td>{{ Age }}</td>
                    <td>{{ City }}</td>
                </tr>
            {{/people}}
            </tbody>
        </table>
    {{/hidePeople}}
</div>